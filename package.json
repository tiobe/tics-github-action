{
  "name": "tics-github-action",
  "version": "2.8.0",
  "description": "The TICS Github action integrates TICS Client analysis to measure your code quality. The incorporated Quality gating feature enables you to analyze and decorate pull requests.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**'",
    "all": "npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiobe/tics-github-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "TIOBE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiobe/tics-github-action/issues"
  },
  "homepage": "https://github.com/tiobe/tics-github-action#readme",
  "dependencies": {
    "@actions/artifact": "^1.1.2",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@octokit/plugin-retry": "^5.0.5",
    "@octokit/request-error": "^5.0.1",
    "@tiobe/http-client": "^0.4.0",
    "@tiobe/install-tics": "^0.5.0",
    "canonical-path": "^1.0.0",
    "compare-versions": "^6.1.0",
    "proxy-agent": "^6.3.1",
    "semver": "^7.5.4",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "@types/underscore": "^1.11.15",
    "@typescript-eslint/parser": "^6.11.0",
    "@vercel/ncc": "^0.38.1",
    "async-listen": "^3.0.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "http-proxy": "^1.18.1",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "prettier": "3.1.0",
    "proxy": "^2.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
